os: mac
-
apple switch: key(cmd-tab)
mission control: key(ctrl-up)
launchpad: key(f4)
dock show: key(ctrl-f3)
dock toggle: key(cmd-alt-d)
find a <user.text>:
  key(ctrl-shift-3)
  insert(text)
glue no format: key(cmd-shift-v)
preferences: key(cmd-,)

(shoomp|super|window) left: key(ctrl-alt-left)
(shoomp|super|window) right: key(ctrl-alt-right)

clear way right: key(ctrl-k)

window (max|maximize): key(ctrl-alt-enter)
